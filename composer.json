{
  "name": "apie/common-apie",
  "description": "Composer package of the apie library: common apie",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\CommonApie\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\CommonApie\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "apie/application-plugin": "dev-main",
    "apie/core": "dev-main",
    "apie/date-time-plugin": "dev-main",
    "apie/pagination-plugin": "dev-main",
    "apie/status-check-plugin": "dev-main",
    "apie/uuid-plugin": "dev-main",
    "apie/value-object-plugin": "dev-main"
  },
  "require-dev": {
    "apie/mock-objects": "dev-main",
    "phpunit/phpunit": "^9.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}